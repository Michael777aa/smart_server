    <!-- http://smartplatforms.org/clinical#Fulfillment -->

    <owl:Class rdf:about="http://smartplatforms.org/clinical#Fulfillment">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/clinical/medication#prescriber"/>
                <owl:onClass rdf:resource="http://smartplatforms.org/anonymous_types/prescriber"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</owl:minQualifiedCardinality>
                <api:doc>
					<rdf:Description>
						<api:name>Clinician who prescribed the medication</api:name>
						<api:description>Prescriber RDF node (see below)</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/clinical/medication#PBM"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                 <api:doc>
					<rdf:Description>
						<api:name>The PBM providing payment for medications</api:name>
						<api:description>PBMID provided by SureScripts (free text)</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/elements/1.1/date"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Date on which medication was dispensed </api:name>
						<api:description>ISO-8601 string </api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/clinical/medication#dispenseQuantity"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Quantity dispensed</api:name>
						<api:description>Integer</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/clinical/medication#pharmacy"/>
                <owl:onClass rdf:resource="http://smartplatforms.org/anonymous_types/pharmacy"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</owl:minQualifiedCardinality>
                <api:doc>
					<rdf:Description>
						<api:name>The pharmacy that dispensed the medication</api:name>
						<api:description>RDF Pharmacy node (see below).</api:description>
					</rdf:Description>
				</api:doc>
			</owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/clinical/medication#dispenseDaysSupply"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>                
                <api:doc>
					<rdf:Description>
						<api:name>The number of days' supply dispensed</api:name>
						<api:description>Integer</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/clinical/medication#dispenseUnits"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Units in which sp:dispenseQuantity is represented </api:name>
						<api:description>Free text</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
     <api:name>Fulfillment</api:name>
     <api:name_plural>Fulfillments</api:name_plural>
     <api:base_path>http://smartplatforms.org/records/{record_id}/fulfillments/{fulfillment_id}</api:base_path>
     <api:description>Each medication may include zero or more sp:fullfillment events, each describing a time when the medication was dispensed by a pharmacy. For example: </api:description>
     <api:example rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"><![CDATA[<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:sp="http://smartplatforms.org/"
  xmlns:core="http://smartplatforms.org/core#"
  xmlns:clin="http://smartplatforms.org/clinical#"
  xmlns:med="http://smartplatforms.org/clinical/medication#"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:v="http://www.w3.org/2006/vcard/ns#">
         <rdf:Description>
            <rdf:type rdf:resource="http://smartplatforms.org/clinical#Fulfillment"/>
            <dc:date>2010-05-12T04:00:00Z</dc:date>
            <med:prescriber>
              <rdf:Description>
                <dcterms:title>Josh Mandel</dcterms:title>
                <clin:NPI>5235235</clin:NPI>
                <clin:DEA>325555555</clin:DEA>
              </rdf:Description>
            </med:prescriber>
            <med:pharmacy>
              <rdf:Description>
                <dcterms:title>CVS #588</dcterms:title>
                <med:NCPDPID>5235235</med:NCPDPID>
                <v:adr>
                  <rdf:Description>
                    <v:street-address>111 Lake Drive</v:street-address>
                    <v:locality>WonderCity</v:locality>
                    <v:postal-code>5555</v:postal-code>
                    <v:country-name>Australia</v:country-name>
                  </rdf:Description>
                </v:adr>
              </rdf:Description>
            </med:pharmacy>
            <med:PBM>T00000000001011</med:PBM>
            <med:dispenseQuantity>30</med:dispenseQuantity>
         </rdf:Description>
</rdf:RDF>]]>
    </api:example>
    </owl:Class>
    
