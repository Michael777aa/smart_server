    <!-- http://smartplatforms.org/clinical#Medication -->

    <owl:Class rdf:about="http://smartplatforms.org/clinical#Medication">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/clinical/medication#drug"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>branded or clinical drug form (RxNorm tty=SCDF or tty=SBDF) concept ID in the RxNorm database. </api:name>
						<api:description>http://link.informatics.stonybrook.edu/rxnorm/RXCUI/{RXCUI}</api:description>
					</rdf:Description>
				</api:doc>                
            </owl:Restriction>
        </rdfs:subClassOf>
       <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/title"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>                
                <api:doc>
					<rdf:Description>
						<api:name>Human-readable name of the drug dispensed</api:name>
						<api:description>Free text</api:description>
					</rdf:Description>
				</api:doc>                
            </owl:Restriction>
        </rdfs:subClassOf> 
       <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/clinical/medication#route"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Route of delivery</api:name>
						<api:description>Free text</api:description>
					</rdf:Description>
				</api:doc>            
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/clinical/medication#instructions"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Clinician-supplied instructions from the prescription signature </api:name>
						<api:description>Free text</api:description>
					</rdf:Description>
				</api:doc>            
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/clinical/medication#startDate"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
