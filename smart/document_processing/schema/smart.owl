    <!-- http://smartplatforms.org/terms#Allergy -->
    
    <owl:Class rdf:about="http://smartplatforms.org/terms#Allergy">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#severity"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <owl:onClass rdf:resource="http://smartplatforms.org/terms#CodedValue"/>
          
				<api:doc>
					<rdf:Description>
						<api:name>Severity of an allergy</api:name>
							<api:description rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"><![CDATA[RDF CodedValue node where code is drawn from the following:

							Code=http://www.ihtsdo.org/snomed-ct/concepts/255604002, Title="Mild"

							Code=http://www.ihtsdo.org/snomed-ct/concepts/371923003, Title="Mild to moderate"

							Code=http://www.ihtsdo.org/snomed-ct/concepts/6736007, Title="Moderate"

							Code=http://www.ihtsdo.org/snomed-ct/concepts/371924009, Title="Moderate to severe"

							Code=http://www.ihtsdo.org/snomed-ct/concepts/24484000, Title="Severe"

							Code=http://www.ihtsdo.org/snomed-ct/concepts/399166001, Title="Fatal"
							]]></api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
        
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#reaction"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <owl:onClass rdf:resource="http://smartplatforms.org/terms#CodedValue"/>
				<api:doc>
					<rdf:Description>
						<api:name>Reaction associated with an allergy</api:name>
						<api:description>RDF CodedValue node drawn from SNOMED-CT</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#category"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <owl:onClass rdf:resource="http://smartplatforms.org/terms#CodedValue"/>
                <api:doc>
				  <rdf:Description>
					<api:name>Category of an allergy</api:name>
					<api:description rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"><![CDATA[RDF CodedValue node where code is drawn from the following:

Code=http://www.ihtsdo.org/snomed-ct/concepts/420134006, Title="propensity to adverse reactions"

Code=http://www.ihtsdo.org/snomed-ct/concepts/418038007, Title="propensity to adverse reactions to substance"

Code=http://www.ihtsdo.org/snomed-ct/concepts/419511003, Title="propensity to adverse reactions to drug"

Code=http://www.ihtsdo.org/snomed-ct/concepts/418471000, Title="propensity to adverse reactions to food"

Code=http://www.ihtsdo.org/snomed-ct/concepts/419199007, Title="allergy to substance"

Code=http://www.ihtsdo.org/snomed-ct/concepts/416098002, Title="drug allergy"

Code=http://www.ihtsdo.org/snomed-ct/concepts/414285001, Title="food allergy"

Code=http://www.ihtsdo.org/snomed-ct/concepts/59037007, Title="drug intolerance"

Code=http://www.ihtsdo.org/snomed-ct/concepts/235719002, Title="food intolerance"
]]></api:description>
				  </rdf:Description>
				</api:doc>
                
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#substance"/>
                <owl:onClass rdf:resource="http://smartplatforms.org/terms#CodedValue"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
				  <rdf:Description>
					<api:name>Substance acting as an allergen</api:name>
					<api:description rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"><![CDATA[RDF CodedValue node.  For drugs, substance is an RxNorm Concept URI:  

http://rxnav.nlm.nih.gov/REST/rxcui/{rxcui}


For environmental and food substance is a UNII:  

http://fda.gov/UNII/{UNII}]]></api:description>
				  </rdf:Description>
				</api:doc>            
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#class"/>
                <owl:onClass rdf:resource="http://smartplatforms.org/terms#CodedValue"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
				  <rdf:Description>
					<api:name>Class of allergen, e.g. Sulfonamides or ACE inhibitors</api:name>
					<api:description>RDF CodedValue node, with code drawn from NDF-RT:  http://rxnav.nlm.nih.gov/REST/rxcui?idtype=NUI&amp;id={NDFRT_ID}</api:description>
				  </rdf:Description>
				</api:doc>            
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#notes"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
				  <rdf:Description>
					<api:name>Additional information associated with this allergy</api:name>
					<api:description>Free text</api:description>
				  </rdf:Description>
				</api:doc>            
            </owl:Restriction>
        </rdfs:subClassOf>
     <api:name>Allergy</api:name>
     <api:name_plural>Allergies</api:name_plural>
	 <api:base_path>/records/{record_id}/allergies/{allergy_id}</api:base_path>
     <api:description rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"><![CDATA[SMArt provides structure for representing alleriges accoring to well-specified standard vocabularies.  A patient with no known allergies produces an AllergyException providing further details (see below). Otherwise, each allergy has a category (drug, food, etc.), a severity, a reaction, and a substance or class of substances.  Since we want these allergies to work as inputs to automated computations, we require specific coding systems to represent substances and classes of substances.  

1.  A drug allergy to a particular drug (e.g. cephalexin) must define a "substance" field with an ingredient-type RxNorm code (tty="IN").  (This is to avoid overly-specific statements like "the patient is allergic to a 500mg cephalexin oral tablet"!) 

2.  A drug allergy to an entire class of drugs (e.g. sulfonamides) must define a "class" field with an NDFRT code for the drug class.

3.  A food or environmental allergy must define a "substance" field with a UNII code.

For instance, below are two allergies:  first, an allergy to the entire class of sulfonamides (note the sp:class predicate and the NDFRT code provided); then, an allergy to a single cephalosporin drug, cephalexin (note the sp:substance predicate and the RxNorm Ingredient CUI provided):]]></api:description>
     <api:example rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"><![CDATA[<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:sp="http://smartplatforms.org/terms#">
      <sp:Allergy>
      <sp:class>
        <sp:CodedValue>
          <sp:code rdf:resource="http://rxnav.nlm.nih.gov/REST/rxcui?idtype=NUI&amp;id=N0000175503" />
          <dcterms:title>Sulfonamide Antibacterial</dcterms:title>
        </sp:CodedValue>
      </sp:class>
      <sp:severity>
        <sp:CodedValue>
          <sp:code rdf:resource="http://www.ihtsdo.org/snomed-ct/concepts/24484000" />
          <dcterms:title>Severe</dcterms:title>
        </sp:CodedValue>
      </sp:severity>
      <sp:reaction>
        <sp:CodedValue>
          <sp:code rdf:resource="http://www.ihtsdo.org/snomed-ct/concepts/39579001" />
          <dcterms:title>Anaphylaxis</dcterms:title>
        </sp:CodedValue>
      </sp:reaction>
      <sp:category>
        <sp:CodedValue>
          <sp:code rdf:resource="http://www.ihtsdo.org/snomed-ct/concepts/416098002" />
          <dcterms:title>Drug allergy</dcterms:title>
        </sp:CodedValue>
      </sp:category>
   </sp:Allergy>

  <sp:Allergy>
      <sp:substance>
        <sp:CodedValue>
          <sp:code rdf:resource="http://rxnav.nlm.nih.gov/REST/rxcui/2231" />
          <dcterms:title>Cephalexin</dcterms:title>
        </sp:CodedValue>
      </sp:substance>
      <sp:severity>
        <sp:CodedValue>
          <sp:code rdf:resource="http://www.ihtsdo.org/snomed-ct/concepts/24484000" />
          <dcterms:title>Severe</dcterms:title>
        </sp:CodedValue>
      </sp:severity>
      <sp:reaction>
        <sp:CodedValue>
          <sp:code rdf:resource="http://www.ihtsdo.org/snomed-ct/concepts/39579001" />
          <dcterms:title>Anaphylaxis</dcterms:title>
        </sp:CodedValue>
      </sp:reaction>
      <sp:category>
        <sp:CodedValue>
          <sp:code rdf:resource="http://www.ihtsdo.org/snomed-ct/concepts/416098002" />
          <dcterms:title>Drug allergy</dcterms:title>
        </sp:CodedValue>
      </sp:category>
   </sp:Allergy>
<sp:Code  rdf:about="http://rxnav.nlm.nih.gov/REST/rxcui?idtype=NUI&amp;id=N0000175503">
   <sp:system rdf:resource="http://rxnav.nlm.nih.gov/REST/rxcui?idtype=NUI" />
   <dcterms:identifier>N0000175503</dcterms:identifier>
</sp:Code>

<sp:Code rdf:about="http://www.ihtsdo.org/snomed-ct/concepts/24484000">
   <sp:system rdf:resource="http://www.ihtsdo.org/snomed-ct/concepts/" />
   <dcterms:identifier>24484000</dcterms:identifier>
</sp:Code>

<sp:Code  rdf:about="http://www.ihtsdo.org/snomed-ct/concepts/39579001">
   <sp:system rdf:resource="http://www.ihtsdo.org/snomed-ct/concepts/" />
   <dcterms:identifier>39579001</dcterms:identifier>
</sp:Code>

<sp:Code  rdf:about="http://www.ihtsdo.org/snomed-ct/concepts/416098002">
   <sp:system rdf:resource="http://www.ihtsdo.org/snomed-ct/concepts/" />
   <dcterms:identifier>416098002</dcterms:identifier>
</sp:Code>

<sp:Code  rdf:about="http://rxnav.nlm.nih.gov/REST/rxcui/2231">
   <sp:system rdf:resource="http://rxnav.nlm.nih.gov/REST/rxcui/" />
   <dcterms:identifier>2231</dcterms:identifier>
</sp:Code>
</rdf:RDF>]]></api:example>
    </owl:Class>

