     <api:name>User</api:name>
     <api:name_plural>Users</api:name_plural>
     <api:description>In RDF/XML, a user of the SMArt container looks like this:</api:description>
     <api:base_path>http://smartplatforms.org/users/{user_id}</api:base_path>
        <api:example rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"><![CDATA[<?xml version="1.0" encoding="utf-8"?>
<rdf:Description rdf:about="http://smartplatforms.org/users/{record_id}">
      <rdf:type rdf:resource="http://smartplatforms.org/user"/>
      <foaf:givenName>Joshua</foaf:givenName>
      <foaf:familyName>Mandel</foaf:familyName>
      <sp:department>Emergency Medicine</sp:department>
      <sp:role>Staff Scientist</sp:role>
      <foaf:mbox>mailto:joshua.mandel@childrens.harvard.edu</foaf:mbox>
   </rdf:Description>
]]></api:example>
  </owl:Class>

