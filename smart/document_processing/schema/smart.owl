    <!-- http://smartplatforms.org/terms#Alert -->
    
    <owl:Class rdf:about="http://smartplatforms.org/terms#Alert">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#severity"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <owl:onClass rdf:resource="http://smartplatforms.org/terms#CodedValue"/>
				<api:doc>
					<rdf:Description>
						<api:name>Nature of the allergy exception</api:name>
							<api:description rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"><![CDATA[RDF CodedValue node where code is drawn from the following:

							Code=http://smartplatforms.org/terms/code/alertLevel#information, Title="Information"

							Code=http://smartplatforms.org/terms/code/alertLevel#warning, Title="Warning"

							Code=http://smartplatforms.org/terms/code/alertLevel#critical, Title="Critical"
							]]></api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#created"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <owl:onClass rdf:resource="http://smartplatforms.org/terms#Attribution"/>
				<api:doc>
					<rdf:Description>
						<api:name>Attribution for which app created this alert, and when.</api:name>
						<api:description>RDF Attribution node</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#acknowledged"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <owl:onClass rdf:resource="http://smartplatforms.org/terms#Attribution"/>
				<api:doc>
					<rdf:Description>
						<api:name>Attribution for who acknowledged this alert, and when.</api:name>
						<api:description>RDF Attribution node</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>

        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#notes"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
				<api:doc>
					<rdf:Description>
						<api:name>Message intended for a human recipient.</api:name>
						<api:description>Free text</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>


    <api:name>Alert</api:name>
     <api:name_plural>Alerts</api:name_plural>
	 <api:base_path>/records/{record_id}/alerts/{alert_id}</api:base_path>
     <api:description>Alerts are a way for an application to generate a message (about a patient record) intended for a human recipient.</api:description>
     <api:example rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"><![CDATA[<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:sp="http://smartplatforms.org/terms#">
<sp:Alert>
  <sp:notes>Critical lab value:  serum sodium 109 mEq/L</sp:notes>
  <sp:severity>
        <sp:CodedValue>
          <sp:code>
            <sp:Code  rdf:about="http://smartplatforms.org/terms/code/alertLevel#critical">
              <sp:system rdf:resource="http://smartplatforms.org/terms/code/alertLevel#" />
              <dcterms:identifier>critical</dcterms:identifier>
            </sp:Code>
          </sp:code>
          <dcterms:title>Critical</dcterms:title>
        </sp:CodedValue>
  </sp:severity>
      <sp:created>
        <sp:Attribution>
          <sp:startTime>2011-03-11T12:05:00</sp:startTime>
          <sp:participant>
             <sp:Participant rdf:about="mailto:lab-alerts@apps.smartplatforms.org"/>
          </sp:participant>
        </sp:Attribution>
