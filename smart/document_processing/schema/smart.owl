    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#comment"/>


    <!-- http://smartplatforms.org/allergy -->
    
    <owl:Class rdf:about="http://smartplatforms.org/allergy">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/allergy/severity"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
				<api:doc>
					<rdf:Description>
						<api:name>Severity of an allergy</api:name>
						<api:description>One of:  http://smartplatforms.org/allergy/severity/mild  http://smartplatforms.org/allergy/severity/moderate http://smartplatforms.org/allergy/severity/severe</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
        
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/allergy/reaction"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
				<api:doc>
					<rdf:Description>
						<api:name>Reaction associated with an allergy</api:name>
						<api:description>Free text.</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/allergy/allergen"/>
                <owl:onClass rdf:resource="http://smartplatforms.org/anonymous_types/allergen"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</owl:minQualifiedCardinality>
				<api:doc>
					<rdf:Description>
						<api:name>Allergen assciated with an allergy</api:name>
						<api:description>Allergen node (see below)</api:description>
					</rdf:Description>
				</api:doc>                
            </owl:Restriction>
        </rdfs:subClassOf>
     <api:name>Allergy</api:name>
     <api:name_plural>Allergies</api:name_plural>
	 <api:base_path>http://smartplatforms.org/records/{record_id}/allergies/{allergy_id}</api:base_path>
     <api:description>Allergies are represented by an allergen, severity, and reaction.  Allergens, in turn, are represented by a category (food, environmental, or drug), substance (the particular allergen), and title (its human-readable name).</api:description>
     <api:example rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"><![CDATA[<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:sp="http://smartplatforms.org/"
  xmlns:foaf="http://xmlns.com/foaf/0.1/"
  xmlns:allergy="http://smartplatforms.org/allergy/">
  <rdf:Description>
      <rdf:type rdf:resource="http://smartplatforms.org/allergy"/>
      <allergy:severity rdf:resource="http://smartplatforms.org/allergy/severity/mild"/>
      <allergy:reaction>Erythema of lower extremities</allergy:reaction>
      <allergy:allergen>
      <rdf:Description>
        <allergy:category rdf:resource="http://smartplatforms.org/allergy/category/food" />
        <allergy:substance rdf:resource="http://fda.gov/UNII/QE1QX6B99R"/>
        <dcterms:title>Peanut</dcterms:title>
      </rdf:Description>
      </allergy:allergen>
   </rdf:Description>
</rdf:RDF>]]></api:example>
    </owl:Class>

