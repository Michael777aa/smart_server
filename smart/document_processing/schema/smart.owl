    <!-- http://smartplatforms.org/medication -->

    <owl:Class rdf:about="http://smartplatforms.org/medication">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/medication#drug"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>branded or clinical drug form (RxNorm tty=SCDF or tty=SBDF) concept ID in the RxNorm database. </api:name>
						<api:description>http://link.informatics.stonybrook.edu/rxnorm/RXCUI/{RXCUI}</api:description>
					</rdf:Description>
				</api:doc>                
            </owl:Restriction>
        </rdfs:subClassOf>
       <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/title"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>                
                <api:doc>
					<rdf:Description>
						<api:name>Human-readable name of the drug dispensed</api:name>
						<api:description>Free text</api:description>
					</rdf:Description>
				</api:doc>                
            </owl:Restriction>
        </rdfs:subClassOf> 
       <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/medication#route"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Route of delivery</api:name>
						<api:description>Free text</api:description>
					</rdf:Description>
				</api:doc>            
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/medication#instructions"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Clinician-supplied instructions from the prescription signature </api:name>
						<api:description>Free text</api:description>
					</rdf:Description>
				</api:doc>            
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/medication#startDate"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>when the patient started taking a medication </api:name>
						<api:description>ISO-8601 string</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/medication#endDate"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>when the patient stopped taking a medication</api:name>
						<api:description>ISO-8601 string</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/medication#doseUnit"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Units in which the dose is expressed</api:name>
						<api:description>Free text</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/medication#strength"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>e.g. the mass of active ingredient in a pill, or the concentration of active ingredient in a suspension </api:name>
						<api:description>Free text</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/medication#strengthUnit"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Units in which the strengh is expressed</api:name>
						<api:description>Free text</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/medication#frequency"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>How often the medication is taken</api:name>
						<api:description>Free text</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/medication#dose"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Dose of medication to take</api:name>
