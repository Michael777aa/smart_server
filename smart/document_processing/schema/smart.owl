    <!-- http://smartplatforms.org/terms#Fulfillment -->

    <owl:Class rdf:about="http://smartplatforms.org/terms#Fulfillment">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#provider"/>
                <owl:onClass rdf:resource="http://smartplatforms.org/terms#Provider"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</owl:minCardinality>
                <api:doc>
					<rdf:Description>
						<api:name>Clinician who prescribed the medication</api:name>
						<api:description>Provider RDF node</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#pbm"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                 <api:doc>
					<rdf:Description>
						<api:name>The PBM providing payment for medications</api:name>
						<api:description>PBMID provided by SureScripts (free text)</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/elements/1.1/date"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Date on which medication was dispensed </api:name>
						<api:description>ISO-8601 string </api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#dispenseQuantity"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Quantity dispensed</api:name>
						<api:description>Integer</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#pharmacy"/>
                <owl:onClass rdf:resource="http://smartplatforms.org/terms#Pharmacy"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</owl:minCardinality>
                <api:doc>
					<rdf:Description>
						<api:name>The pharmacy that dispensed the medication</api:name>
						<api:description>RDF Pharmacy node.</api:description>
					</rdf:Description>
				</api:doc>
			</owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#dispenseDaysSupply"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>                
                <api:doc>
					<rdf:Description>
						<api:name>The number of days' supply dispensed</api:name>
						<api:description>Integer</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#dispenseUnits"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Units in which sp:dispenseQuantity is represented </api:name>
						<api:description>Free text</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
     <api:name>Fulfillment</api:name>
     <api:name_plural>Fulfillments</api:name_plural>
     <api:base_path>http://smartplatforms.org/records/{record_id}/fulfillments/{fulfillment_id}</api:base_path>
     <api:description>Each medication may include zero or more sp:fullfillment events, each describing a time when the medication was dispensed by a pharmacy. For example: </api:description>
     <api:example rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"><![CDATA[<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:sp="http://smartplatforms.org/terms#"
  xmlns:foaf="http://xmlns.com/foaf/0.1/"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:v="http://www.w3.org/2006/vcard/ns#">
 <sp:Fulfillment>
    <dc:date>2010-05-12T04:00:00Z</dc:date>
    <sp:provider>
      <sp:Provider>
        <foaf:name>Joshua Mandel</foaf:name>
        <sp:npi>5235235</sp:npi>
        <sp:dea>325555555</sp:dea>
      </sp:Provider>
    </sp:provider>
    <sp:pharmacy>
      <sp:Pharmacy>
        <sp:ncpdpid>5235235</sp:ncpdpid>
            <foaf:name>CVS #588</foaf:name>
            <sp:address>
              <sp:Address>
                <v:street-address>111 Lake Drive</v:street-address>
                <v:locality>WonderCity</v:locality>
                <v:postal-code>5555</v:postal-code>
                <v:country-name>Australia</v:country-name>
              </sp:Address>
              </sp:address>
      </sp:Pharmacy>
    </sp:pharmacy>
    <sp:pbm>T00000000001011</sp:pbm>
    <sp:dispenseQuantity>60</sp:dispenseQuantity>
    <sp:dispenseDaysSupply>30</sp:dispenseDaysSupply>
 </sp:Fulfillment>
</rdf:RDF>]]>
    </api:example>
    </owl:Class>
    
