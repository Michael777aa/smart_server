    <!-- http://smartplatforms.org/fulfillment -->

    <owl:Class rdf:about="http://smartplatforms.org/fulfillment">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/prescriber"/>
                <owl:onClass rdf:resource="http://smartplatforms.org/anonymous_types/prescriber"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</owl:minQualifiedCardinality>
                <api:doc>
					<rdf:Description>
						<api:name>Clinician who prescribed the medication</api:name>
						<api:description>Prescriber RDF node (see below)</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/PBM"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                 <api:doc>
					<rdf:Description>
						<api:name>The PBM providing payment for medications</api:name>
						<api:description>PBMID provided by SureScripts (free text)</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/elements/1.1/date"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Date on which medication was dispensed </api:name>
						<api:description>ISO-8601 string </api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/dispenseQuantity"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Quantity dispensed</api:name>
						<api:description>Integer</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/pharmacy"/>
                <owl:onClass rdf:resource="http://smartplatforms.org/anonymous_types/pharmacy"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</owl:minQualifiedCardinality>
                <api:doc>
					<rdf:Description>
						<api:name>The pharmacy that dispensed the medication</api:name>
						<api:description>RDF Pharmacy node (see below).</api:description>
					</rdf:Description>
				</api:doc>
			</owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/dispenseDaysSupply"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>                
                <api:doc>
					<rdf:Description>
						<api:name>The number of days' supply dispensed</api:name>
						<api:description>Integer</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/dispenseUnits"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Units in which sp:dispenseQuantity is represented </api:name>
						<api:description>Free text</api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>
     <api:name>Fulfillment</api:name>
     <api:name_plural>Fulfillments</api:name_plural>
     <api:base_path>http://smartplatforms.org/records/{record_id}/medications/{medication_id}/fulfillments/{fulfillment_id}</api:base_path>
     <api:description>Each medication may include zero or more sp:fullfillment events, each describing a time when the medication was dispensed by a pharmacy. For example: </api:description>
     <api:example rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"><![CDATA[<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:sp="http://smartplatforms.org/"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:v="http://www.w3.org/2006/vcard/ns#">
         <rdf:Description>
            <rdf:type rdf:resource="http://smartplatforms.org/fulfillment"/>
            <dc:date>2010-05-12T04:00:00Z</dc:date>
            <sp:prescriber>
              <rdf:Description>
                <dcterms:title>Josh Mandel</dcterms:title>
                <sp:NPI>5235235</sp:NPI>
                <sp:DEA>325555555</sp:DEA>
              </rdf:Description>
            </sp:prescriber>
            <sp:pharmacy>
              <rdf:Description>
                <dcterms:title>CVS #588</dcterms:title>
                <sp:NCPDPID>5235235</sp:NCPDPID>
                <v:adr>
                  <rdf:Description>
                    <v:street-address>111 Lake Drive</v:street-address>
                    <v:locality>WonderCity</v:locality>
                    <v:postal-code>5555</v:postal-code>
                    <v:country-name>Australia</v:country-name>
                  </rdf:Description>
                </v:adr>
              </rdf:Description>
            </sp:pharmacy>
            <sp:PBM>T00000000001011</sp:PBM>
            <sp:dispenseQuantity>30</sp:dispenseQuantity>
         </rdf:Description>
</rdf:RDF>]]>
    </api:example>
    </owl:Class>
    
