            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/capability"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
                <api:doc>
					<rdf:Description>
						<api:name>Capability of a container</api:name>
						<api:description>Capability URI (e.g. http://smartplatforms.org/capability/Pillbox/lookup) </api:description>
					</rdf:Description>
				</api:doc>
            </owl:Restriction>
        </rdfs:subClassOf>

     <api:name>Capabilities</api:name>
     <api:name_plural>Capabilities</api:name_plural>
     <api:description>A SMArt Container's capabilities are reported as an RDF graph. So far only three capabilities have been defined, as shown in the example below. These are:

    * lookup in SNOMED
    * lookup in FDA Structured Product Labels
    * lookup in NLM Pillbox.</api:description>
<api:base_path>http://smartplatforms.org/capabilities/</api:base_path>        
        <api:example rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"><![CDATA[<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:sp="http://smartplatforms.org/">
  <rdf:Description rdf:about="http://localhost:7000">
    <rdf:type rdf:resource="http://smartplatforms.org/container"/>
    <sp:capability rdf:resource="http://smartplatforms.org/capability/SNOMED/lookup"/>
    <sp:capability rdf:resource="http://smartplatforms.org/capability/SPL/lookup"/>
    <sp:capability rdf:resource="http://smartplatforms.org/capability/Pillbox/lookup"/>
  </rdf:Description>
</rdf:RDF>
]]></api:example>
  </owl:Class>

<api:call>
  <api:description>Get a list of all auxiliiary container capabilities</api:description>
  <api:method>GET</api:method>
  <api:path>http://smartplatforms.org/capabilities/</api:path>
  <api:target rdf:resource="http://smartplatforms.org/container"/>
  <api:category>container_items</api:category>
</api:call>


