<?xml version="1.0" encoding="utf-8" ?>
<Records>
{% for record in records %}
  <Record id="{{ record.id }}" label="{{record.fn}} {{record.ln}}" >
  <demographics>
  <firstname>{{record.fn}}</firstname>
  <lastname>{{record.ln}}</lastname>
  <dob>{{record.dob}}</dob>
  <gender>{{record.gender}}</gender>
  <zipcode>{{record.zipcode}}</zipcode>
  </demographics>
  </Record>
{% endfor %}
</Records>
