<?xml version="1.0" encoding="utf-8" ?>
{% if phas %}
<Apps>
{% for pha in phas %}
<App id="{{pha.email}}">
  <startURLTemplate>{{pha.start_url_template}}</startURLTemplate>
  <name>{{pha.name}}</name>
  <description>{{pha.description}}</description>
  <frameable>{{pha.frameable}}</frameable>
  <ui>{{pha.has_ui}}</ui>
</App>
{% endfor %}
</Apps>
{% else %}
<Apps/>
{% endif %}
